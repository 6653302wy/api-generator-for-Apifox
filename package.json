{
	"name": "generate-api-for-apifox",
	"displayName": "generate-api",
	"description": "generate api(ts) for Apifox/右键创建api请求和定义文件",
	"version": "0.0.1",
	"keywords": [
		"apifox",
		"typescript",
		"api",
		"swagger",
		"swagger3",
		"自动",
		"生成"
	],
	"publisher": "wanpp",
	"repository": {
		"type": "git",
		"url": "https://github.com/6653302wy/api-generator-for-Apifox"
	},
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:generate-apis"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "generate-apis",
				"title": "生成API文件(ts)"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "generate-apis",
					"group": "2_workspace@2"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack --config ./build/node-extension.webpack.config.js",
		"watch": "webpack --watch --config ./build/node-extension.webpack.config.js",
		"package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"ts-loader": "^8.0.11",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"webpack": "^5.10.0",
		"webpack-cli": "^4.2.0"
	},
	"dependencies": {
		"axios": "^1.7.9"
	}
}